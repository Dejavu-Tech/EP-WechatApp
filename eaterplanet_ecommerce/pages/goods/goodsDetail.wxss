    z-index: 100;
    width: 80rpx;
}

.fixed-service {
    display: inline-block;
    width: 80rpx;
    height: 80rpx;
    background: #e7614d93;
    backdrop-filter: blur(40rpx);
    border-radius: 20rpx 20rpx 20rpx 20rpx;
    text-align: center;
    color: #fff;
    box-sizing: border-box;
    font-size: 20rpx;
    padding: 10rpx;
    line-height: 1.2;
    z-index: 100;
    margin-bottom: 10rpx;
}

.fixed-service::after {
    border: 0;
}

.fixed-service .iconfont {
    font-size: 30rpx;
}

.fixed-share {
    display: inline-block;
    transform: translateY(950rpx);
