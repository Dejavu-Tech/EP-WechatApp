.modal-foot1 {
    position: relative;
    padding: 20rpx 0rpx;
}

.modal-head::after,.modal-foot::before {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
	transform: scaleY(.5);
}

.modal-head::after {
    border-bottom: 0px solid #c8c7cc;
    bottom: 0;
}

.modal-foot::before {
    border-top: 0px solid #c8c7cc;
    top: 0;
}

